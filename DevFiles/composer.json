{
    "name": "tads/meuspontos",
    "authors": [
        {
            "name": "Adenilton Barroso",
            "email": "adenilton.barroso@yahoo.com.br"
        }
    ],
    "require": {
        "php": ">=5.5",
        "respect/rest": "dev-master",
        "twig/twig": ">=1.0@stable",
        "doctrine/orm": ">=2.5",
        "symfony/yaml": "2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8@stable",
        "squizlabs/php_codesniffer": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "": "src/",
            "Entity\\": "Entity/",
            "Controllers\\": "Controllers/",
            "Boundary\\": "Boundary/",
            "Traits\\": "Traits/",
            "Library\\": "Library/"
        }
    },
    "config":{
        "bin-dir": "vendor/bin"
    },
    "minimum-stability": "dev"
}